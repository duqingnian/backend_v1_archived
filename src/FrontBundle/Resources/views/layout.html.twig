<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>{% block title %}TITLE{% endblock %}</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport"
        content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <link rel="stylesheet" href="{{ asset('ui_core/layui/css/layui.css') }}" media="all">
    <link rel="stylesheet" href="{{ asset('ui_core/style/admin.css') }}" media="all">
	<script>
		{% set _route = app.request.attributes.get('_route') %}
		var _route="{{ _route }}";
	</script>
	<script src="{{ asset('ui_core/layui/layui.js') }}"></script>
</head>
<body class="layui-layout-body">
	{% block content %}{% endblock %}
</body>
</html>